{
    "version": 1,
    "projects": {
        "core": {
            "projectType": "library",
            "root": "packages/core",
            "sourceRoot": "packages/core/src",
            "prefix": "aspect",
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "packages/core/tsconfig.lib.json",
                            "packages/core/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!packages/core/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/core/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "schematics": {}
        },
        "example": {
            "projectType": "application",
            "schematics": {},
            "root": "packages/example",
            "sourceRoot": "packages/example/src",
            "prefix": "aspect",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/packages/example",
                        "index": "packages/example/src/index.html",
                        "main": "packages/example/src/main.ts",
                        "polyfills": "packages/example/src/polyfills.ts",
                        "tsConfig": "packages/example/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "packages/example/src/favicon.ico",
                            "packages/example/src/assets"
                        ],
                        "styles": ["packages/example/src/styles.css"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "packages/example/src/environments/environment.ts",
                                    "with": "packages/example/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "example:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "example:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "example:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "packages/example/tsconfig.app.json",
                            "packages/example/tsconfig.spec.json",
                            "packages/example/tsconfig.editor.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!packages/example/**/*"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/example/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "store": {
            "projectType": "library",
            "root": "packages/store",
            "sourceRoot": "packages/store/src",
            "prefix": "aspect",
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "packages/store/tsconfig.lib.json",
                            "packages/store/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!packages/store/**/*"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/store/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "schematics": {}
        },
        "fsm": {
            "projectType": "library",
            "root": "packages/fsm",
            "sourceRoot": "packages/fsm/src",
            "prefix": "aspect",
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "packages/fsm/tsconfig.lib.json",
                            "packages/fsm/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!packages/fsm/**/*"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "packages/fsm/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "schematics": {}
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "analytics": false
    },
    "defaultProject": "core",
    "schematics": {
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "none"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    }
}
