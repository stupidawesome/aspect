{
    "npmScope": "aspect",
    "affected": {
        "defaultBase": "master"
    },
    "implicitDependencies": {
        "workspace.json": "*",
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        "tsconfig.base.json": "*",
        "tslint.json": "*",
        ".eslintrc.json": "*",
        "nx.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "projects": {
        "core": {
            "tags": []
        },
        "example": {
            "tags": []
        },
        "store": {
            "tags": []
        },
        "fsm": {
            "tags": []
        }
    },
    "workspaceLayout": {
        "appsDir": "packages",
        "libsDir": "packages"
    }
}
